<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <ControlTheme x:Key="MaterialAccessText"
                TargetType="AccessText">
    <Setter Property="FontFamily" Value="{StaticResource MaterialDesignFonts}" />
  </ControlTheme>

  <ControlTheme x:Key="{x:Type AccessText}" TargetType="AccessText"
                BasedOn="{StaticResource MaterialAccessText}" />
</ResourceDictionary>
