<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <Styles.Resources>
    <ResourceDictionary>
      <ResourceDictionary.MergedDictionaries>
        <ResourceInclude Source="avares://Material.Styles/Resources/Themes/Separator.axaml" />
      </ResourceDictionary.MergedDictionaries>
    </ResourceDictionary>
  </Styles.Resources>

  <Style Selector="Separator.middle, Separator.Middle">
    <Setter Property="Theme" Value="{StaticResource MiddleSeparator}" />
  </Style>

  <Style Selector="Separator.vertical, Separator.Vertical">
    <Setter Property="Theme" Value="{StaticResource VerticalSeparator}" />
  </Style>
</Styles>
