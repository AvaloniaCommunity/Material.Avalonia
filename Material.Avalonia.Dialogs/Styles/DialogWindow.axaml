<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:viewModels="clr-namespace:Material.Dialog.ViewModels" >
    <StyleInclude Source="avares://Material.Avalonia.Dialogs/Styles/EmbeddedDialogControl.axaml"/>

    <Style Selector="Window" x:DataType="viewModels:DialogWindowViewModel">
        <Setter Property="Title" Value="{Binding WindowTitle}" />
        <Setter Property="WindowStartupLocation" Value="{Binding WindowStartupLocation}" />
    </Style>
</Styles>